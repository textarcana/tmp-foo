#!/usr/bin/env bash

set -Eaeuo pipefail -o functrace

watch -c '
echo;
echo perl version;
echo;
bin/fsm test/inputs/basic.log | tee test/opt/1.txt;
echo;
echo bash version;
echo;
sh vendor/read_lines_from_file.sh test/inputs/basic.log | tee test/opt/2.txt; echo;
echo diff of outputs:;
echo; diff -u test/1.txt test/2.txt'
